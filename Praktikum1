			};
			for (a=0;a<=7;a++) {
				for (b=0;b<=37;b++) {
					if (matriks [a][b] == 1) {
						System.out.print("*");
					}
					else if (matriks [a] [b] == 0) {
						System.out.print(" ");
					}
				}
